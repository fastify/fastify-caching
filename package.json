{
  "name": "fastify-caching",
  "version": "6.2.0",
  "description": "A plugin for Fastify to enable management of cache control headers",
  "main": "plugin.js",
  "scripts": {
    "lint": "standard --verbose | snazzy",
    "lint:ci": "standard",
    "test": "npm run unit",
    "unit": "tap -J \"test/*.test.js\"",
    "unit:report": "npm run unit -- --coverage-report=html",
    "unit:verbose": "npm run unit -- -Rspec"
  },
  "precommit": [
    "lint",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/fastify/fastify-caching.git"
  },
  "keywords": [
    "fastify",
    "cache",
    "caching"
  ],
  "author": "James Sumners <james.sumners@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fastify/fastify-caching/issues"
  },
  "homepage": "https://github.com/fastify/fastify-caching#readme",
  "devDependencies": {
    "fastify": "^3.27.1",
    "pre-commit": "^1.2.2",
    "snazzy": "^9.0.0",
    "standard": "^16.0.4",
    "tap": "^15.1.6"
  },
  "dependencies": {
    "abstract-cache": "^1.0.1",
    "fastify-plugin": "^3.0.1",
    "uid-safe": "^2.1.5"
  }
}
